<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="94.0" prefWidth="307.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.logmonitor.LogMonitorController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button fx:id="btnChooseLog" onAction="#chooseLog" text="Open">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <VBox.margin>
         <Insets />
      </VBox.margin></Button>
   <TextField fx:id="fullPathToLogFile" onAction="#chooseLog" />
   <Button fx:id="btnChoosePlaceAndMakeReport" mnemonicParsing="false" onAction="#choosePlaceAndMakeReport" text="Make report" />
</VBox>
